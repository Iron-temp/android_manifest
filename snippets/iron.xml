<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />
  
  <remote  name="gitlab"
           fetch="https://gitlab.com" />
  
  <remote name="iron"
          fetch="https://github.com/IronOSProject"
          revision="twelve"/>
  
  <remote name="github" fetch="https://github.com" />
  <remote name="github-ssh" fetch="ssh://git@github.com" />
  <remote name="gitlab" fetch="https://gitlab.com" />
  <remote name="gitlab-ssh" fetch="ssh://git@gitlab.com" />
  <remote name="proton" fetch="https://github.com/ProtonAOSP" revision="sc" />
  <remote name="proton-ssh" fetch="ssh://git@github.com/ProtonAOSP" revision="sc" />
  <remote name="blobs" fetch="https://github.com/NeutronBlobs" revision="sc" />
  <remote name="blobs-ssh" fetch="ssh://git@github.com/NeutronBlobs" revision="sc" />


  <!-- Rootdir -->
  <project path="art" name="android_art" remote="iron" />
  <project path="bionic" name="android_bionic" groups="pdk" remote="iron" revision="iron-12.1" />
  <project path="libcore" name="android_libcore" remote="iron" revision="iron-12.1" />
  <project path="manifest" name="android_manifest" remote="iron" />

  <!-- Bootable -->
  <project path="bootable/recovery" name="android_bootable_recovery" groups="pdk" remote="iron" revision="iron-12.1" />

  <!-- Build -->
  <project path="build/make" name="android_build_make" groups="pdk" remote="iron" revision="iron-12.1" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed" remote="iron" revision="iron-12.1" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project path="device/iron/sepolicy" name="android_device_iron_sepolicy" remote="iron" />
  <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" groups="qcom,pdk-qcom" remote="iron" />
  <project path="device/qcom/sepolicy_vndr" name="android_device_qcom_sepolicy_vndr" groups="qcom,pdk-qcom" remote="iron" />
  <project path="device/qcom/sepolicy-legacy" name="android_device_qcom_sepolicy" groups="qcom,pdk-qcom" revision="twelve-legacy" remote="iron" />
  <project path="device/qcom/sepolicy-legacy-um" name="android_device_qcom_sepolicy" groups="qcom,pdk-qcom" revision="twelve-legacy-um" remote="iron" />

  <!-- External -->
  <project path="external/tinycompress" name="android_external_tinycompress" groups="pdk" remote="iron" />
  <project name="android_external_noto-fonts" path="external/noto-fonts" remote="iron" revision="iron-12.1" />

  <!-- Frameworks -->
  <project name="android_frameworks_av" path="frameworks/av" remote="iron" revision="iron-12.1" />
  <project name="android_frameworks_base" path="frameworks/base" remote="iron" revision="iron-12.1" />
  <project name="android_frameworks_native" path="frameworks/native" remote="iron" revision="iron-12.1" />
  
  <!-- Hardware -->
<!--  <project path="hardware/interfaces" name="android_hardware_interfaces" remote="iron" /> -->
  <project path="hardware/lineage/interfaces" name="android_hardware_lineage_interfaces" remote="iron" />
  <project path="hardware/qcom-caf/sm8150/display" name="hardware_qcom-caf_sm8150_display" remote="iron" />

  <!-- Packages -->
  <project name="android_packages_apps_Settings" path="packages/apps/Settings" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_Boxer" path="packages/apps/Boxer" remote="iron" />
  <project name="android_packages_apps_ThemePicker" path="packages/apps/ThemePicker" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_WallpaperPicker2" path="packages/apps/WallpaperPicker2" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_SettingsIntelligence" path="packages/apps/SettingsIntelligence" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_Launcher3" path="packages/apps/Launcher3" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_Dialer" path="packages/apps/Dialer" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_DeskClock" path="packages/apps/DeskClock" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_Contacts" path="packages/apps/Contacts" remote="iron" revision="iron-12.1" />
  <project name="android_packages_apps_Messaging" path="packages/apps/Messaging" remote="proton" groups="proton" />
  <project name="android_packages_apps_Camera2" path="packages/apps/Camera2" remote="proton" groups="proton" />
  <project name="android_packages_modules_Connectivity" path="packages/modules/Connectivity" remote="iron" revision="iron-12.1" />
  <project name="android_packages_modules_Permission" path="packages/modules/Permission" remote="iron" revision="iron-12.1" />
  <project name="android_packages_modules_Wifi" path="packages/modules/Wifi" remote="iron" revision="iron-12.1" />
  <project name="android_packages_inputmethods_LatinIME" path="packages/inputmethods/LatinIME" remote="iron" revision="iron-12.1" />

  <!-- Prebuilds -->
  <project path="prebuilts/abi-dumps/vndk" name="android_prebuilts_abi-dumps_vndk" groups="pdk" remote="iron" revision="iron-12.1" />

  <!-- System -->
  <project path="system/core" name="android_system_core" groups="pdk" remote="iron" revision="iron-12.1" />
  <project path="system/libhwbinder" name="android_system_libhwbinder" remote="iron" />
  <project path="system/sepolicy" name="android_system_sepolicy" remote="iron" revision="iron-12.1" />
  <project path="system/update_engine" name="android_system_update_engine" remote="iron" />

  <!-- Vendor -->
  <project name="android_vendor_iron" path="vendor/iron" remote="iron" />
  <project name="android_vendor_support" path="vendor/support" remote="iron" />

</manifest>
